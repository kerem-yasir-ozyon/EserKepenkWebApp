@model List<GuestViewModel>


<table class="table table-striped table-hover">
    <tr>
        <td>#</td>
        <td>Misafir Ad</td>
        <td>Misafir Soyadı</td>
        <td>Misafir Telefon No</td>
        <td>Misafir E-Posta</td>
        <td>Misafir Mesajı</td>GuestMessages
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.RowNum</td>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Phone</td>
                <td>@item.Email</td>
                <td>@item.Description</td>
            </tr>
        }
    }
</table>

